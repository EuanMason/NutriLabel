<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="masonator117.com.nutrilabel.dataProcessing">

    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">



        <TableRow
            android:id="@+id/energyRow"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <TextView
                android:id="@+id/timelineenergyText"
                android:layout_width="245dp"
                android:layout_height="wrap_content"
                android:text="@string/energy"
                android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium" />

            <TextView
                android:id="@+id/energyOutput"
                android:layout_width="match_parent"
                android:layout_height="match_parent" /><![CDATA[





            />


        ]]>
        </TableRow>

        <TableRow
            android:id="@+id/fatRow"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <TextView
                android:id="@+id/timelinefatText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/fat"
                android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium" />

            <TextView
                android:id="@+id/fatOutput"
                android:layout_width="match_parent"
                android:layout_height="match_parent" /><![CDATA[

            />
        ]]>
        </TableRow>

        <TableRow
            android:id="@+id/saturatesRow"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <TextView
                android:id="@+id/saturatesText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/saturates"
                android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium" />

            <TextView
                android:id="@+id/saturatesOutput"
                android:layout_width="match_parent"
                android:layout_height="match_parent" /><![CDATA[

            />
        ]]>
        </TableRow>

        <TableRow
            android:id="@+id/sugarsRow"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <TextView
                android:id="@+id/sugarsText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sugars"
                android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium" />

            <TextView
                android:id="@+id/sugarsOutput"
                android:layout_width="match_parent"
                android:layout_height="match_parent" /><![CDATA[



            />

        ]]>
        </TableRow>

        <TableRow
            android:id="@+id/saltsRow"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <TextView
                android:id="@+id/saltsText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/salts"
                android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium" />

            <TextView
                android:id="@+id/saltsOutput"
                android:layout_width="match_parent"
                android:layout_height="match_parent" /><![CDATA[

            />
        ]]>
        </TableRow>

        <TableRow
            android:id="@+id/portionRow"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <TextView
                android:id="@+id/portionText"
                android:layout_width="245dp"
                android:layout_height="wrap_content"
                android:text="@string/portion"
                android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium" />

            <Spinner
                android:id="@+id/spinnerDecimal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <Spinner
                android:id="@+id/spinnerFraction"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" /><![CDATA[













            />






        ]]>
        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/testingText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/done"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/editData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:text="Edit data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>
